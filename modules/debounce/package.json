{
  "name": "@wetransfer/concorde-debounce",
  "version": "1.2.0",
  "description": "Debounce functions for regular and async functions.",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "NODE_ENV=production ../../node_modules/.bin/rollup -c",
    "build:watch": "../../node_modules/.bin/watch 'npm run build' src",
    "format": "../../node_modules/.bin/prettier-standard '(__test__|src)/**/*.js'",
    "lint": "../../node_modules/.bin/standard",
    "lint:fix": "../../node_modules/.bin/standard --fix",
    "prepublish": "npm run build",
    "test": "../../node_modules/.bin/jest",
    "test:watch": "../../node_modules/.bin/jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WeTransfer/concorde.js.git"
  },
  "keywords": [
    "async",
    "debounce",
    "wetransfer"
  ],
  "author": "WeTransfer folks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WeTransfer/concorde.js/issues"
  },
  "homepage": "https://github.com/WeTransfer/concorde.js#readme",
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "setupFiles": [
      "../../scripts/jest/setup-environment.js"
    ]
  },
  "standard": {
    "ignore": [
      "**/dist/"
    ],
    "globals": [
      "afterEach",
      "beforeEach",
      "describe",
      "expect",
      "it",
      "jest"
    ]
  }
}
